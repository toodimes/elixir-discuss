<%= form_for @changeset, topic_path(@conn, :create), fn f -> %>
   <div class="row form-group">
        <div class="input-field col s12">
            <%= text_input f, :title %>
            <%= error_tag f, :title %>
            <label for="title">Title</label>
        </div>
   </div>

   <%= submit "Create Topic", class: "waves-effect waves-light btn-large" %>
<% end %>